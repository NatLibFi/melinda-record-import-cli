{
	"name": "@natlibfi/melinda-record-import-cli",
	"description": "CLI for Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://www.npmjs.com/package/@natlibfi/melinda-record-import-cli",
	"bugs": {
		"url": "https://github.com/NatLibFi/melinda-record-import-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NatLibFi/melinda-record-import-cli.git"
	},
	"license": "mit",
	"version": "4.0.1",
	"main": "./dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=22"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"melinda-record-import-cli": "src/index.js"
	},
	"scripts": {
		"prepare": "npm run build",
		"lint": "eslint --fix src",
		"test": "npm run lint",
		"build": "esbuild \"src/*.js\"  --sourcemap --platform=node --outdir=dist --analyze",
		"build:dev": "npm run build",
		"watch": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src --exec 'npm test'"
	},
	"dependencies": {
		"@natlibfi/melinda-backend-commons": "3.0.4",
		"@natlibfi/melinda-commons": "14.0.2",
		"@natlibfi/melinda-record-import-commons": "^13.0.3",
		"http-status": "^2.1.0",
		"moment": "^2.30.1",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"cross-env": "^10.1.0",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2"
	}
}