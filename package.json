{
	"name": "@natlibfi/melinda-record-import-cli",
	"description": "CLI for Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-record-import-cli",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-record-import-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-record-import-cli.git"
	},
	"license": "mit",
	"version": "3.0.6",
	"main": "./dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=22"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": "./dist/index.js",
	"scripts": {
		"prepare": "npm run build",
		"lint": "eslint --fix src",
		"test": "npm run lint",
		"build": "esbuild \"src/*.js\"  --sourcemap --platform=node --outdir=dist --analyze",
		"build:dev": "npm run build",
		"watch": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src --exec 'npm test'"
	},
	"dependencies": {
		"@natlibfi/melinda-backend-commons": "^3.0.0-alpha.4",
		"@natlibfi/melinda-commons": "^14.0.0-alpha.3",
		"@natlibfi/melinda-record-import-commons": "13.0.0-alpha.5",
		"http-status": "^2.1.0",
		"moment": "^2.29.4",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"cross-env": "^10.0.0",
		"esbuild": "^0.25.9",
		"eslint": "^9.34.0"
	}
}