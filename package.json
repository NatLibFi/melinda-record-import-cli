{
	"name": "@natlibfi/melinda-record-import-cli",
	"description": "CLI for Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-record-import-cli",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-record-import-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-record-import-cli.git"
	},
	"license": "mit",
	"version": "3.0.6",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": "./dist/index.js",
	"scripts": {
		"prepare": "npm run build",
		"lint": "eslint --fix src",
		"test": "npm run lint",
		"build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist",
		"build:dev": "npm run build",
		"watch": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src --exec 'npm test'"
	},
	"dependencies": {
		"@babel/runtime": "^7.23.6",
		"@natlibfi/melinda-backend-commons": "^2.3.1-alpha.3",
		"@natlibfi/melinda-commons": "^13.0.15",
		"@natlibfi/melinda-record-import-commons": "^11.0.0-alpha.41",
		"http-status": "^1.7.4",
		"mime": "^3.0.0",
		"moment": "^2.29.4",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.6",
		"@babel/node": "^7.22.19",
		"@babel/preset-env": "^7.23.6",
		"@natlibfi/eslint-config-melinda-backend": "^3.0.5",
		"cross-env": "^7.0.3",
		"eslint": "^8.49.0",
		"nodemon": "^3.1.4"
	},
	"eslintConfig": {
		"parser": "@babel/eslint-parser",
		"extends": [
			"@natlibfi/melinda-backend"
		]
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": "maintained node versions"
				}
			]
		]
	}
}