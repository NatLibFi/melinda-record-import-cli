{
	"name": "@natlibfi/melinda-record-import-cli",
	"description": "CLI for Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-record-import-cli",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-record-import-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-record-import-cli.git"
	},
	"license": "AGPL-3.0+",
	"version": "2.0.7",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": "./dist/index.js",
	"scripts": {
		"lint": "eslint --fix src",
		"test": "npm run lint",
		"build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist",
		"build:dev": "npm run build",
		"watch": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src --exec 'npm test'"
	},
	"dependencies": {
		"@babel/core": "^7.15.5",
		"@natlibfi/melinda-backend-commons": "^2.0.3",
		"@natlibfi/melinda-record-import-commons": "^9.1.0",
		"http-status": "^1.5.0",
		"mime": "^2.5.1",
		"moment": "^2.29.1",
		"yargs": "^17.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/eslint-parser": "^7.15.7",
		"@babel/node": "^7.15.4",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.15.6",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"@natlibfi/eslint-config-melinda-backend": "^1.0.6",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.13"
	},
	"eslintConfig": {
		"parser": "@babel/eslint-parser",
		"extends": [
			"@natlibfi/melinda-backend"
		]
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "14"
					}
				}
			]
		],
		"plugins": [
			"@babel/plugin-syntax-dynamic-import",
			"dynamic-import-node"
		]
	}
}
